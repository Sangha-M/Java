//Area of a circle
class Main {
  public static void main(String[] args) {
  
  final double pi=3.141592; 
  double r=4.5;
   double answer = pi*(r*r);
    System.out.println("Area of a circle is "+ answer);

    double x = 7.7;
    double area = pi*(x*x);
    System.out.println("Areas of Circle 2 is "+ area);
  }
}
