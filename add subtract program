  public static void main(String[] args) {
    int a; // declaration
    a=7; //initialization
  int x=7; 
  int y=2;
    
    System.out.print(x+y);
    System.out.println("");
    System.out.println(x*y);
     System.out.println("");
      System.out.println(x/y);
    System.out.println("");
      System.out.println(x-y);
    
